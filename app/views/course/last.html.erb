<h1>テストお疲れ様！</h1>
<p>
    <%= @msg %>
</p>
    <table>
        <% $answer_table.each do |a_t| %>
        <tr>
            <p>
                <%= a_t[0] + a_t[1] %>
            <p>
            <p>
                <%= "答え:" + a_t[2] + "    あなたの解答:" + a_t[3] %>
            </p>
            <p>
                <%= a_t[4] %>
            </p>
        </tr>
        <% end %>
        
    </table>
<p>
    <%= button_to("選択画面へ", '/choose_course', :method => :get) %>
</p>